@model TP.Site.Models.Organization.StoreModel

<div class="form-group form-group-boder">  
    @Html.LabelFor(m => m.UniqueCode, new {@class="control-label col-lg-2"})    
    <div class="col-lg-6">
        @Html.TextBoxFor(m => m.UniqueCode, "", new { @class = "form-control",id="txtUniqueCode"})
    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.UniqueCode)
        </p>
    </div>
</div>
<div class="form-group form-group-boder">
    @Html.LabelFor(m => m.Name, new { @class = "control-label col-lg-2" })
    <div class="col-lg-6">
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control", id = "txtName" })
    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.Name)
        </p>
    </div>
</div>

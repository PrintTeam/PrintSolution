    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.Address)
        </p>
    </div>
</div>
<div class="form-group form-group-boder">
    @Html.LabelFor(m => m.Telephone, new { @class = "control-label col-lg-2" })
    <div class="col-lg-6">
        @Html.TextBoxFor(m => m.Telephone, new { @class = "form-control", id = "txtTelephone" })
    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.Telephone)
        </p>
    </div>
</div>
<div class="form-group form-group-boder">
    @Html.LabelFor(m => m.ResponsiblePerson, new { @class = "control-label col-lg-2" })
    <div class="col-lg-6">
        @Html.TextBoxFor(m => m.ResponsiblePerson, new { @class = "form-control", id = "txtResponsiblePerson" })
    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.ResponsiblePerson)
        </p>
    </div>
</div>
<div class="form-group form-group-boder">
    @Html.LabelFor(m => m.Description, new { @class = "control-label col-lg-2" })
    <div class="col-lg-6">
        @Html.TextAreaFor(m => m.Description, 5, 10, new { @class = "form-control", id = "txtDescription" })
    </div>
    <div class="col-lg-4">
        <p class="text-danger">
            @Html.ValidationMessageFor(m => m.Description)
        </p>
    </div>
</div>